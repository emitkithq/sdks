{
  "name": "@emitkit/js",
  "version": "1.0.0",
  "description": "Official TypeScript/JavaScript SDK for EmitKit API",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "generate": "openapi-ts",
    "build": "pnpm run generate && tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "tsc --noEmit",
    "clean": "rm -rf dist .turbo node_modules src/generated",
    "prepublishOnly": "pnpm run build && pnpm run test"
  },
  "keywords": [
    "emitkit",
    "events",
    "notifications",
    "api",
    "sdk",
    "typescript",
    "javascript",
    "real-time"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/emitkit/emitkit-sdks.git",
    "directory": "packages/js"
  },
  "bugs": {
    "url": "https://github.com/emitkit/emitkit-sdks/issues"
  },
  "homepage": "https://emitkit.com",
  "license": "MIT",
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.88.0",
    "@types/node": "^22.10.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^4.0.15"
  },
  "publishConfig": {
    "access": "public"
  }
}